pipeline{
    agent any
    stages{
        stage('Checkout the code from GitHub'){
            steps{
                 git url: 'https://github.com/AbeerSaxena/star-agile-health-care/'
                 echo 'github url checkout'
            }
        }
        stage('Code Compile'){
            steps{
                echo 'starting compiling'
                sh 'mvn compile'
            }
        }
        stage('Code Testing '){
            steps{
                sh 'mvn test'
            }
        }
        stage('QA'){
            steps{
                sh 'mvn checkstyle:checkstyle'
            }
        }
        stage('Package'){
            steps{
                sh 'mvn package'
            }
        }
          
    }
}
